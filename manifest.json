{
	"_version": "0.0.1",
	"sap.app":{
		"_version": "0.0.1",
		"id": "be.fiddle.BusinessObjectModel",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "0.0.1"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"dataSources": {
			"monsters": {
				"uri": "/service/db.php?entity=monsters",
				"type": "JSON",
				"settings": {
					"annotations": []
				}
			}
		},
		"offline":true
	},
	"sap.ui": {
		"_version": "1.1.0",
		"technology": "UI5",
		"icons": {
			"icon": "",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		},
		"supportedThemes": ["sap_bluecrystal", "sap_belize_plus"]
	},
	"sap.ui5": {
		"_version": "1.1.0",
		"rootView": {
			"viewName": "be.fiddle.BusinessObjectModel.view.Monsters",
			"type": "XML",
			"id":"Monsters"
		},
		"dependencies": {
			"minUI5Version": "1.30.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.ui.layout": {}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "be.fiddle.BusinessObjectModel.i18n.i18n"
				}
			},
			"monsters": {
				"type": "be.fiddle.BusinessObjectModel.model.BusinessObjectModel",
				"dataSource": "monsters",
				"settings": {
					"testData": "/service/monsters.json",
                    "ObjectClass":"be.fiddle.BusinessObjectModel.model.businessobject.Monster",
                    "db":"ui5busobjModel",
                    "store":"monsters",
					"metadataUrlParams": {
						"sap-documentation": "heading"
					}
				}
			}
		},
		"resources": {
			"css": []
		}
	}
}